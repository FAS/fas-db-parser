{
  "name": "Kleptomaniac",
  "version": "1.1.0",
  "description": "Automatic fetching and parsing products from FAS database",
  "homepage": "https://github.com/FAS/fas-db-parser",
  "author": "Mr. Hyde <npm@2bad.me>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/FAS/fas-db-parser.git"
  },
  "bugs": {
    "url": "https://github.com/FAS/fas-db-parser/issues"
  },
  "license": "Copyright 2015 LotusTM",
  "keywords": [
    "parsing",
    "scrapping",
    "request"
  ],
  "type": "module",
  "main": "dist/fas-db-parser.js",
  "jsnext:main": "index.js",
  "bin": {
    "klpt": "build/bin/klpt.js"
  },
  "files": [
    "source",
    "dist"
  ],
  "dependencies": {
    "bluebird": "3.5.5",
    "cheerio": "0.22.0",
    "commander": "^3.0.0",
    "he": "1.2.0",
    "intel": "1.2.0",
    "jsonfile": "5.0.0",
    "request": "2.88.0",
    "requestretry": "4.0.0",
    "sort-object": "3.0.2",
    "striptags": "3.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "esm": "3.2.25",
    "rimraf": "2.7.1",
    "snazzy": "8.0.0",
    "standard": "13.1.0"
  },
  "scripts": {
    "prestart": "rimraf logs build && mkdir logs build && npm run build",
    "start": "node -r esm source/bin/klpt.js",
    "build": "babel source -d build",
    "test": "standard --verbose | snazzy"
  },
  "engines": {
    "node": ">=7.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "debug": true
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
