{
  "name": "Kleptomaniac",
  "version": "1.0.1",
  "description": "Automatic fetching and parsing products from FAS database",
  "homepage": "https://github.com/FAS/fas-db-parser",
  "author": "Mr. Hyde <npm@2bad.me>",
  "main": "dist/fas-db-parser.js",
  "jsnext:main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/FAS/fas-db-parser.git"
  },
  "bugs": {
    "url": "https://github.com/FAS/fas-db-parser/issues"
  },
  "license": "Copyright 2015 LotusTM",
  "keywords": [
    "parsing",
    "scrapping",
    "request"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "bluebird": "3.5.0",
    "cheerio": "0.22.0",
    "he": "1.1.1",
    "intel": "1.1.2",
    "jsonfile": "2.4.0",
    "request": "2.81.0",
    "requestretry": "1.12.0",
    "sort-object": "3.0.2",
    "striptags": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.2.1",
    "snazzy": "6.0.0",
    "standard": "10.0.0-beta.0"
  },
  "scripts": {
    "prestart": "rm -rf logs build && mkdir logs build && npm run build",
    "start": "node --max_old_space_size=6144 build/index.js",
    "build": "babel source -d build",
    "test": "standard --verbose | snazzy"
  },
  "engines": {
    "node": ">=7.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "debug": true
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
